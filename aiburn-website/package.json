{
  "name": "aiburn-calculator",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "update-prices": "node scripts/fetch-ai-prices.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:prices": "jest scripts/__tests__/fetch-ai-prices.test.js",
    "test:calculations": "jest calculations.test.js",
    "test:validation": "jest validation.test.js",
    "test:integration": "jest priceIntegration.test.js",
    "test:ui": "jest ui.test.js",
    "test:e2e": "jest e2e-workflows.test.js",
    "test:all": "jest --coverage --verbose",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "a11y": "playwright test e2e/accessibility.spec.js",
    "a11y:ui": "playwright test e2e/accessibility.spec.js --ui",
    "a11y:report": "playwright show-report",
    "smoke-test": "node scripts/smoke-test.js",
    "load-test": "node scripts/load-test.js",
    "k6": "echo 'Running K6 load test (requires k6 installed). Usage: k6 run k6-load-test-simple.js' && echo 'Install K6: brew install k6'",
    "k6:prod": "k6 run k6-load-test-simple.js -e BASE_URL=https://aiburn.howstud.io",
    "k6:local": "k6 run k6-load-test-simple.js -e BASE_URL=http://localhost:5173",
    "security:audit": "npm audit --registry https://registry.npmjs.org/ --audit-level=high",
    "lint": "eslint src --ext .js,.jsx || true"
  },
  "dependencies": {
    "@sentry/react": "^8.18.0",
    "dompurify": "^3.3.0",
    "lucide-react": "^0.394.0",
    "nodemailer": "^7.0.10",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^6.30.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/react": "^15.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "vite": "^7.2.2"
  }
}
